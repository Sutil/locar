<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<lifecycle>
		<phase-listener>br.com.locar.app.login.LoginErrorPhaseListener</phase-listener>
	</lifecycle>
	<converter>
		<converter-id>permissaoConverter</converter-id>
		<converter-class>br.com.locar.app.converter.PermissaoConverter</converter-class>
	</converter>
	<converter>
		<converter-id>perfilUsuarioConverter</converter-id>
		<converter-class>br.com.locar.app.converter.PerfilUsuarioConverter</converter-class>
	</converter>
	<converter>
		<converter-id>usuarioConverter</converter-id>
		<converter-class>br.com.locar.app.converter.UsuarioConverter</converter-class>
	</converter>
	<converter>
		<converter-id>exemplarConverter</converter-id>
		<converter-class>br.com.locar.app.converter.ExemplarConverter</converter-class>
	</converter>
	
</faces-config>